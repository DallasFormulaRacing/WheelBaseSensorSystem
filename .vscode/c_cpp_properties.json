{
    "version": 4,
    "configurations": [
        {
            "name": "STM32",
            "configurationProvider": "ms-vscode.cmake-tools",
            "intelliSenseMode": "${default}",
            "includePath": [
                "${workspaceFolder}/CM7/Core/Inc",
                "${workspaceFolder}/CM4/Core/Inc",
                "${workspaceFolder}/Drivers/CMSIS/Include",
                "${workspaceFolder}/Drivers/CMSIS/Device/ST/STM32H7xx/Include",
                "${workspaceFolder}/Drivers/STM32H7xx_HAL_Driver/Inc",
                "${workspaceFolder}/Drivers/BSP/STM32H7xx_Nucleo"
            ],
            "defines": [
                "STM32H755xx",
                "USE_HAL_DRIVER"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}/CM7/Core/Inc",
                    "${workspaceFolder}/CM4/Core/Inc",
                    "${workspaceFolder}/Drivers"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ]
}
